{
  "name": "typescript-ddd-shop",
  "version": "1.0.0",
  "engines": {
    "node": "^20.9"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "node ./dist/src/apps/shop/backend/index.js",
    "dev:shop:backend": "NODE_ENV=dev ts-node-dev --ignore-watch node_modules -r tsconfig-paths/register ./src/apps/shop/backend/start.ts",
    "dev:backoffice:backend": "NODE_ENV=dev ts-node-dev --ignore-watch node_modules -r tsconfig-paths/register ./src/apps/backoffice/backend/start.ts",
    "dev:importer": "NODE_ENV=dev ts-node-dev --ignore-watch node_modules -r tsconfig-paths/register ./src/apps/importer/start.ts",
    "lint": "eslint --fix .",
    "postinstall": "npm run build",
    "test": "mocha --config ./tests/.mocharc.json ${MOCHA_ARGS}",
    "test-watch": "mocha -r ts-node/register -r tsconfig-paths/register tests/**/*.spec.ts --watch --extensions ts",
    "test:features": "npm run test:shop:backend:features && npm run test:backoffice:backend:features",
    "test:shop:backend:features": "NODE_ENV=test RABBITMQ_HOSTNAME=rabbitmq_test cucumber-js -p shop_backend --exit",
    "test:backoffice:backend:features": "NODE_ENV=test RABBITMQ_HOSTNAME=rabbitmq_test cucumber-js -p backoffice_backend --exit",
    "command:store:rabbitmq": "NODE_ENV=production ts-node src/apps/shop/backend/command/run-configure-rabbitmq-command"
  },
  "_moduleAliases": {
    "@domain": "dist/src/shared/domain",
    "@config": "dist/src/config",
    "@constants": "dist/src/shared/constants",
    "@infrastructure": "dist/src/shared/infrastructure",
    "@interfaces": "dist/src/interfaces",
    "@shop-backend": "dist/src/contexts/shop/backend",
    "@shop-backend-app": "dist/src/apps/shop/backend",
    "@backoffice-backend": "dist/@backoffice-backend/backend",
    "@backoffice-backend-app": "dist/src/apps/backoffice/backend",
    "@shared": "dist/src/shared"
  },
  "dependencies": {
    "amqplib": "^0.10.4",
    "bcryptjs": "^2.4.3",
    "bodybuilder": "^2.5.1",
    "convict": "^6.2.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-promise-router": "^4.1.1",
    "fp-ts": "^2.16.5",
    "fs": "^0.0.1-security",
    "helmet": "^5.1.1",
    "http-status": "^1.7.4",
    "http-status-codes": "^2.3.0",
    "io-ts": "^2.2.21",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "mongodb": "^4.17.2",
    "node-dependency-injection": "^3.1.2",
    "nodemon": "^2.0.22",
    "reflect-metadata": "^0.1.14",
    "uuid": "^8.3.2",
    "uuid-validate": "^0.0.3",
    "winston": "^3.13.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.5"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^9.6.0",
    "@faker-js/faker": "^7.6.0",
    "@elastic/elasticsearch": "^8.13.1",
    "@types/amqplib": "^0.10.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/chai": "4.3.4",
    "@types/chai-as-promised": "^7.1.8",
    "@types/convict": "^6.1.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/glob": "^8.1.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.17.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.31",
    "@types/sinon": "10.0.13",
    "@types/sinon-chai": "3.2.9",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^8.3.4",
    "@types/uuid-validate": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.4.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-ban": "^1.6.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-floating-promise": "^1.0.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-html": "^8.1.1",
    "expect-type": "^0.16.0",
    "mocha": "^10.4.0",
    "prettier": "^3.2.5",
    "sinon": "^16.1.1",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}