{
  "name": "ts-bookstore-api",
  "version": "1.0.0",
  "engines": {
    "node": "17.6"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "node ./dist/src/apps/store/backend/index.js",
    "dev": "NODE_ENV=dev ts-node-dev --ignore-watch node_modules -r tsconfig-paths/register ./src/apps/store/backend/index.ts",
    "dev:importer": "NODE_ENV=dev ts-node-dev --ignore-watch node_modules -r tsconfig-paths/register ./src/apps/store/importer/index.ts",
    "lint": "eslint --fix .",
    "postinstall": "npm run build",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register test/**/*.spec.ts --timeout 60000 --exit"
  },
  "_moduleAliases": {
    "@core": "dist/src/shared/core",
    "@config": "dist/src/config",
    "@constants": "dist/src/shared/constants",
    "@infrastructure": "dist/src/shared/infrastructure",
    "@interfaces": "dist/src/interfaces",
    "@storeback": "dist/src/contexts/store/backend",
    "@storebackapp": "dist/src/apps/store/backend",
    "@shared": "dist/src/shared"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/uuid": "^8.3.4",
    "@types/uuid-validate": "^0.0.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
