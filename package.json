    "helmet": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "ioredis": "4.19.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mongodb": "^4.8.0",
    "nodemon": "^2.0.19",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2",
    "uuid-validate": "^0.0.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
