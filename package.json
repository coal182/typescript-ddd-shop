{
  "name": "typescript-ddd-shop",
  "version": "1.0.0",
  "engines": {
    "node": "^18.16"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "node ./dist/src/apps/shop/backend/index.js",
    "dev:shop:backend": "NODE_ENV=dev ts-node-dev --ignore-watch node_modules -r tsconfig-paths/register ./src/apps/shop/backend/start.ts",
    "dev:backoffice:backend": "NODE_ENV=dev ts-node-dev --ignore-watch node_modules -r tsconfig-paths/register ./src/apps/backoffice/backend/start.ts",
    "dev:importer": "NODE_ENV=dev ts-node-dev --ignore-watch node_modules -r tsconfig-paths/register ./src/apps/shop/importer/index.ts",
    "lint": "eslint --fix .",
    "postinstall": "npm run build",
    "test": "mocha --config ./tests/.mocharc.json ${MOCHA_ARGS}",
    "test-watch": "mocha -r ts-node/register -r tsconfig-paths/register tests/**/*.spec.ts --watch --extensions ts",
    "test:features": "npm run test:shop:backend:features && npm run test:backoffice:backend:features",
    "test:shop:backend:features": "NODE_ENV=test RABBITMQ_HOSTNAME=rabbitmq_test cucumber-js -p shop_backend --exit",
    "test:backoffice:backend:features": "NODE_ENV=test RABBITMQ_HOSTNAME=rabbitmq_test cucumber-js -p backoffice_backend --exit",
    "command:store:rabbitmq": "NODE_ENV=production ts-node src/apps/shop/backend/command/run-configure-rabbitmq-command"
  },
  "_moduleAliases": {
    "@domain": "dist/src/shared/domain",
    "@config": "dist/src/config",
    "@constants": "dist/src/shared/constants",
    "@infrastructure": "dist/src/shared/infrastructure",
    "@interfaces": "dist/src/interfaces",
    "@shop-backend": "dist/src/contexts/shop/backend",
    "@shop-backend-app": "dist/src/apps/shop/backend",
    "@backoffice-backend": "dist/@backoffice-backend/backend",
    "@backoffice-backend-app": "dist/src/apps/backoffice/backend",
    "@shared": "dist/src/shared"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.9.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/uuid": "^8.3.4",
    "@types/uuid-validate": "^0.0.1",
    "amqplib": "^0.10.3",
    "bcryptjs": "^2.4.3",
    "bodybuilder": "^2.5.1",
    "class-transformer": "^0.5.1",
    "convict": "^6.2.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-promise-router": "^4.1.1",
    "fs": "^0.0.1-security",
    "helmet": "^5.1.0",
    "http-status": "^1.6.2",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mongodb": "^4.8.0",
    "node-dependency-injection": "^3.1.0",
    "nodemon": "^2.0.19",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2",
    "uuid-validate": "^0.0.3",
    "winston": "^3.8.2",
    "xlsx": "^0.18.5",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^9.2.0",
    "@faker-js/faker": "^7.3.0",
    "@types/amqplib": "^0.10.1",
    "@types/chai": "4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/convict": "^6.1.1",
    "@types/cucumber": "^7.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.7.14",
    "@types/sinon": "10.0.13",
    "@types/sinon-chai": "3.2.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "cucumber": "^6.0.7",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.2.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "sinon": "^15.0.1",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "ts-sinon": "2.0.2",
    "tsconfig-paths": "^4.0.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.7.4",
    "yarn-upgrade-all": "^0.7.1"
  }
}
